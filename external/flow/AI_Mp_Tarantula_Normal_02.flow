start > pos_front
	pos_front y skill2_usable
		skill2_usable y skill2_near
		skill2_usable n hp_>=_50
	pos_front n hp_>=_50
		hp_>=_50 y skill1_usable
			skill1_usable y skill1_light
			skill1_usable n cell_skill1
				cell_skill1 y move_slot1
				cell_skill1 n wait
		hp_>=_50 n pos!_front
			pos!_front y movable_front
				movable_front y move_front
				movable_front n skill1_usable
					skill1_usable y skill1_light
					skill1_usable n cell_skill1
						cell_skill1 y move_slot1
						cell_skill1 n wait
			pos!_front n wait
